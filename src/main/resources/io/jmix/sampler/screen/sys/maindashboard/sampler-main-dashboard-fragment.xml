<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://jmix.io/schema/ui/window">
    <layout expand="dashboardScrollBox"
            margin="true"
            responsive="true"
            spacing="true">
        <hbox id="header"
              visible="false"
              width="100%">
            <hbox id="historyBox"
                  height="30px"/>
        </hbox>
        <scrollBox id="dashboardScrollBox">
            <cssLayout id="cssLayout"
                       responsive="true"
                       stylename="main-dashboard-csslayout-responsive"
                       width="100%"/>
        </scrollBox>
    </layout>
</window>
